{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@interface/*": [
                "src/interfaces/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@img/*": [
                "src/assets/images/*"
            ],
            "@icons/*": [
                "src/assets/icons/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@components/*": [
                "src/components/*",
            ],
            "@HOC/*": [
                "src/HOC/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@constants/*": [
                "src/constants/*"
            ],
            "@services/*": [
                "src/services/*"
            ]
        }
    }
}